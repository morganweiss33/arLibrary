var dataParams = {
	proxyScheme: undefined /* STRING */,
	headers: undefined /* JSON */,
	ignoreSSLErrors: true /* BOOLEAN */,
	useNTLM: undefined /* BOOLEAN */,
	workstation: undefined /* STRING */,
	useProxy: undefined /* BOOLEAN */,
	withCookies: undefined /* BOOLEAN */,
	proxyHost: undefined /* STRING */,
	url: "https://github.com/morganweiss33/arLibrary/blob/master/MOCK_DATA.json" /* STRING */,
	timeout: undefined /* NUMBER */,
	proxyPort: undefined /* INTEGER */,
	password: undefined /* STRING */,
	domain: undefined /* STRING */,
	username: undefined /* STRING */
    //infoTableName : "InfoTable",
    //dataShapeName : "book-data"
};

// result: JSON
var data = Resources["ContentLoaderFunctions"].GetJSON(dataParams);

//logger.debug('Result' +JSON[0].array.length);


var params1 = {
    infoTableName : "InfoTable",
    dataShapeName : "book-data"
};

// CreateInfoTableFromDataShape(infoTableName:STRING("InfoTable"), dataShapeName:STRING):INFOTABLE(book-data)
var myInfoTable = Resources["InfoTableFunctions"].CreateInfoTableFromDataShape(params1);

for (var i=0; i < data.array.length;i++)
{
    myInfoTable.AddRow({isbn:data[i].isbn, bookTitle:data[i].bookTitle, author:data[i].author});
}

var result = myInfoTable;

